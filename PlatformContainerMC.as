package {		import flash.display.MovieClip;	import PlatformMC;		public class PlatformContainerMC extends MovieClip {				public function PlatformContainerMC() {											}				public function initialisePlatforms($collisionArea) {							for (var i:int = 0; i < this.numChildren;i ++) 			{				var child = getChildAt(i);				//trace(child.hasOwnProperty("collisionArea")); 				if (child.hasOwnProperty("collisionArea")) 				{					child.collisionArea = $collisionArea;				}			}		}			}	}